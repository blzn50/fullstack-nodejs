<%- include('../includes/head.ejs')%>
</head>

<body>
  <%- include('../includes/navigation.ejs')%>
  <main>
    <% if(products.length > 0) { %>
    <ul>
      <% products.forEach(cartProd => { %>
      <li><%= cartProd.productData.title%> (<%= cartProd.qty%>)</li>
      <%})%>
    </ul>
    <% } else { %>
    <h1>The cart is empty!</h1>
    <% } %>
  </main>
  <%- include('../includes/end.ejs')%>
